<!DOCTYPE html>
<html>
<head>
<link rel="icon"
      type="image/png"
      href="<%= image_url('favicon.png')%>">
  <title>ChronicleReStarter</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
  <script src="//widget.cloudinary.com/global/all.js" type="text/javascript"></script>
  <script type="text/javascript">
    window.CLOUDINARY_OPTIONS = {
      cloud_name: "<%=ENV['CLOUD_NAME']%>",
      upload_preset: "<%= ENV['UPLOAD_PRESET'] %>"
    };
    window.left_arrow = '<%= image_url('left_arrow.svg') %>';
    window.right_arrow = '<%= image_url('right_arrow.svg') %>';
    window.logout = '<%= image_url('logout.png') %>';
    window.logo = '<%= image_url('logo.png') %>';
    window.search = '<%= image_url('search.png') %>';
    window.down = '<%= image_url('down_arrow.svg') %>';
    window.profile_pic = '<%= image_url('default_prof_pic.jpg') %>';
  </script>
</head>
<body>

<%= yield %>

</body>
</html>
